{
  "name": "mern-auth-starter",
  "version": "1.0.0",
  "description": "My MERN+auth boilerplate",
  "main": "index.js",
  "scripts": {
    "start": "cd server && babel-node --presets env server.js",
    "start:dev": "concurrently --kill-others-on-fail \"yarn start\" \"cd server/client && yarn start\"",
    "heroku-postbuild": "cd server/client && yarn install && yarn run build"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "8.0.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "csvtojson": "^2.0.8",
    "cuid": "^2.1.4",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "limax": "^1.7.0",
    "mongoose": "^5.3.15",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "nodemon": "^1.18.7",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "randomstring": "^1.1.5",
    "sanitize-html": "^1.19.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noahbuscher/mern-auth-starter.git"
